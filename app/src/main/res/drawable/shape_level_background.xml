<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/shape_gradient" >
        <shape
            android:shape="rectangle"
            android:useLevel="false">

            <corners
                android:radius="4dp" />

            <gradient
                android:startColor="@color/color_ABBED1"
                android:endColor="@color/color_ABBED1"
                android:type="linear"
                android:useLevel="false" />
        </shape>
    </item>

    <item android:id="@+id/shape_stroke">
        <shape
            android:shape="rectangle"
            android:useLevel="false">

            <corners
                android:radius="4dp" />

            <stroke
                android:width="1dp"
                android:color="@color/color_1A000000"/>
        </shape>
    </item>
</layer-list>
